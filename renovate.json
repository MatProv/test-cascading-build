{
    "endpoint":"https://api.github.com",
    "platform":"github",
    "renovateFork":true,
    "includeForks":true,
    "labels":[
       "renovate",
       "dependencies",
       "automated"
    ],
    "onboarding":true,
    "onboardingConfig":{
       "extends":[
          "config:base",
          "docker:enableMajor"
       ]
    },
    "docker":{
        "packageRules": [
          {
            "packageNames": [
              "matprov/base-img-test"
            ],
            "groupName": "base image update"
         }
        ]
    },
    "extends":[
       "config:base",
       "docker:enableMajor"
    ],
    "repositories":[
       "MatProv/test-cascading-build"
    ],
    "prHourlyLimit":0,
    "prConcurrentLimit": 20,
    "logFile":"renovate.log",
    "logFileLevel":"debug",
    "logLevel":"info",
    "schedule": [
        "every 1 hour"
    ]
  }